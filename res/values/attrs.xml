<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 左右滑动按钮SlideButton自定义属性 -->
    <declare-styleable name="slide_button">
        <attr name="slideButtonStyle" format="reference"></attr>
        <attr name="colorOpened" format="color|reference" />
        <attr name="colorClosed" format="color|reference" />
        <attr name="colorBall" format="color|reference" />
        <attr name="slidable" format="boolean" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="rect_circle" value="2" />
            <enum name="circle" value="3" />
        </attr>
    </declare-styleable>
</resources>
